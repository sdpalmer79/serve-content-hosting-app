{
  "hosting": [
    {
      "target": "serve-content",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/site{,/**}",
          "run": {
            "serviceId": "site-content",
            "region": "me-west1"
          }
        },
        {
          "source": "**",
          "run": {
            "serviceId": "site-content",
            "region": "me-west1"
          }
        }
      ]
    },
    {
      "target": "pennyroots-verbaworks",
      "public": "public/pennyroots",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "redirects": [
        {
          "source": "/",
          "destination": "https://pennyroots.verbaworks.com/site?id=66af88036b1bbfdf9d9f33bb",
          "type": 301
        }
      ],
      "rewrites": [
        {
          "source": "/sitemap.xml",
